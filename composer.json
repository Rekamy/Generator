{
    "name": "rekamy/generator",
    "description": "Web & API Generator with Swagger API Doc",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "request",
        "model",
        "crud",
        "repository",
        "controller",
        "designpattern",
        "view",
        "generator",
        "swagger"
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "laravel/framework": "^8.0 || ^7.0 || ^9.0",
        "doctrine/dbal": "^2.9 || ^3.0.0"
    },
    "require-dev": {},
    "suggest": {
        "prettus/l5-repository": "Needed for using repository patterns",
        "laravel/passport": "Needed for authentication and authorization",
        "spatie/laravel-permission": "Needed for granular authorization control",
        "darkaonline/l5-swagger": "Needed to provide swagger api documentation"
    },
    "authors": [
        {
            "name": "Firdaus Ishaddin",
            "email": "firdausishaddin@rekamy.com"
        },
        {
            "name": "Zahrin Azam",
            "email": "zahrin.azam@rekamy.com"
        },
        {
            "name": "Shahrin Nidzam",
            "email": "shahrin.nidzam@rekamy.com"
        },
        {
            "name": "Razlan Razak",
            "email": "razlanrzk@rekamy.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rekamy\\Generator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rekamy\\Generator\\GeneratorServiceProvider"
            ]
        }
    }
}
